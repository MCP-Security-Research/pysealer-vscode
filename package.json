{
  "name": "pysealer-vscode",
  "displayName": "pysealer",
  "description": "A Visual Studio Code extension with support for the pysealer cryptographic signature tool",
  "version": "0.6.0",
  "publisher": "pysealer",
  "icon": "icon.png",
  "license": "MIT",
  "keywords": [
    "rust",
    "python",
    "decorator",
    "cryptography"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MCP-Security-Research/pysealer-vscode.git"
  },
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pysealer.initProject",
        "title": "Pysealer: Initialize Pysealer Project"
      },
      {
        "command": "pysealer.lockFile",
        "title": "Pysealer: Lock Current File"
      },
      {
        "command": "pysealer.checkFile",
        "title": "Pysealer: Check Current File"
      },
      {
        "command": "pysealer.removeFile",
        "title": "Pysealer: Remove Seals from Current File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle",
    "bundle": "npm run bundle:python && npm run compile",
    "bundle:python": "python3 scripts/install_bundled_libs.py",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
